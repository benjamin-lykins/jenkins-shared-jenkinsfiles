// Parameters for the build
properties([
  parameters([
     stringParam(name: 'Name', defaultValue: ""),
     boolParam(name: 'Name', defaultValue: true),
  ])
])

pipeline {

  agent any

  stages {

    stage('Echo') {

      steps {
        
         echo 'Hello World!'
         echo 'Jenkinsfile1'

      }

    }

}

}